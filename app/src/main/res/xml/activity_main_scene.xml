            motion:layout_constraintTop_toTopOf="@+id/guideline5"
            android:layout_width="wrap_content"
            android:id="@+id/imageView4"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_editor_absoluteX="57dp"
            motion:layout_constraintEnd_toStartOf="@id/imageView" />
        <Constraint
            android:id="@+id/guideline6"
            motion:layout_constraintGuide_percent="0.8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="@+id/guideline5"
            android:layout_width="wrap_content"
            android:id="@+id/imageView5"
            motion:layout_editor_absoluteX="32dp"
            motion:layout_constraintStart_toEndOf="@id/imageView"
            motion:layout_constraintEnd_toStartOf="@id/imageView2" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="@+id/guideline5"
            android:layout_width="wrap_content"
            android:id="@+id/imageView6"
            motion:layout_editor_absoluteX="64dp"
